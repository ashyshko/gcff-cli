{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "camelcase": "off",
    "unicorn/import-style": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "varsIgnorePattern": "^_"
      }
    ]
  },
  "overrides": [
    {
      "files": [
        "test/**"
      ],
      "rules": {
        "@typescript-eslint/no-unused-vars": [
          "off"
        ],
        "max-nested-callbacks": [
          "off"
        ],
        "@typescript-eslint/no-empty-function": [
          "off"
        ]
      }
    }
  ]
}