{
  "name": "gcff",
  "version": "0.1.1",
  "description": "GCFF CLI: Deploy your web pages and API services easily with cloud function technology",
  "keywords": [
    "GCFF CLI",
    "Deploy",
    "Web pages",
    "API services",
    "Cloud function technology",
    "Command-line tool",
    "Deployment automation",
    "Serverless",
    "Cloud functions",
    "Web development",
    "API deployment",
    "Easy deployment",
    "Cloud computing",
    "Function-as-a-Service",
    "Web hosting",
    "Serverless architecture",
    "Developer tool",
    "Cloud platform",
    "Service deployment",
    "Web application deployment",
    "Function orchestration",
    "Rapid deployment",
    "Infrastructure management",
    "Cloud-based services"
  ],
  "author": "Andrey Shishko",
  "bin": {
    "gcff": "./bin/run"
  },
  "homepage": "https://github.com/ashyshko/gcff-cli#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ashyshko/gcff-cli.git"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@google-cloud/storage": "^6.12.0",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "adm-zip": "^0.5.10",
    "chalk": "^4.1.2",
    "google-auth-library": "^8.9.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.26",
    "@types/adm-zip": "^0.5.0",
    "@types/chai": "^4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/decompress": "^4.2.4",
    "@types/mocha": "^9.0.0",
    "@types/mock-require": "^2.0.1",
    "@types/node": "^16.18.38",
    "chai": "^4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "oclif": "^3",
    "shx": "^0.3.3",
    "sinon": "^15.2.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "gcff",
    "dirname": "gcff",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {},
    "macos": {
      "identifier": "com.gcff.cli"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "test:watch": "while :; do nyc mocha; read; done",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/ashyshko/gcff-cli/issues",
  "types": "dist/index.d.ts"
}